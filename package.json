    "Safari",
    "Exokit"
  ],
  "author": "William Murphy",
  "license": "MIT",
  "peerDependencies": {
    "aframe": "^0.8.2",
    "networked-aframe": "^0.6.1"
  },
  "devDependencies": {
    "aframe": "^0.8.2",
    "concurrently": "^4.0.1",
    "networked-aframe": "^0.6.1",
    "standard": "^12.0.1",
    "watchify": "^3.11.0"
  }
}
